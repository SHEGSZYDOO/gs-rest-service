public class EmployeeManager {
    public Employees initializeEmployees() {
        Employees employees = new Employees();
        
        // Hard-code example employees
        employees.getEmployeeList().add(new Employee("1", "John", "Doe", "john.doe@example.com", "Manager"));
        employees.getEmployeeList().add(new Employee("2", "Jane", "Smith", "jane.smith@example.com", "Engineer"));
        // Add more employees as needed
        
        return employees;
    }
}
